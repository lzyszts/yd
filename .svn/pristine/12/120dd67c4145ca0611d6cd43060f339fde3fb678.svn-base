<script type="text/javascript">
    $('body').scrollspy({ target: '.right-navbar' })
    // 右边小箭头
    $(".rollBtn").click(function(){
        $(".right-navbar").show().removeClass('fadeOutRight').addClass('fadeInRight');
        $(".shade").show();
    });
    $(".shade").click(function(){
        $(this).hide();
        $(".right-navbar").removeClass("fadeInRight").addClass('fadeOutRight');
    });


</script>


