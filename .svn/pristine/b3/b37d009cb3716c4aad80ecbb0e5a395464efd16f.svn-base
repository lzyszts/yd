}


/*****nav*****/
.navbar{
	margin-bottom: 0;
	border-radius: 0px;
}
.topnav{
	background-color: #846638;
}
.nav a{
	color: #fff;
}
.nav a:hover{
	background-color:transparent !important;
}
.nav a:focus{
	background-color:transparent !important;
}
.nav ul li::before{
	content: "";
	position: absolute;
	top: -7px;
	left: 0;
	width: 0;
	height: 100%;
	border-bottom: 2px solid #f8c301;
	transition: 0.2s all linear;
}
.nav .active::before{
	width: 100%;
}
.nav ul li:hover::before{
	/*width: 100%;*/
}
.navbar-brand{
	padding: 10px 15px;
}
.menuBtn {
	display: block;
	margin-top: 2px;
	width: 50px;
	height: 50px;
	float: right;
	position: relative;
	overflow: hidden;
	cursor: pointer;
}
.menuBtn .line {
	width: 29px;
	height: 2px;
	background: #fff;
	position: absolute;
	left: 50%;
	margin-left: -18px;
}
.menuBtn .line{
	transition:0.4s all;
}
.menuBtn .line1 {
	transform: rotate(0deg);
	-ms-transform:rotate(0deg);
	-moz-transform:rotate(0deg);
	-webkit-transform:rotate(0deg);
	-o-transform:rotate(0deg);
	top: 14px;
}
.menuBtn .line2 {
	top: 24px;
}.menuBtn .line3 {
	transform: rotate(0deg);
	-ms-transform:rotate(0deg);
	-moz-transform:rotate(0deg);
	-webkit-transform:rotate(0deg);
	-o-transform:rotate(0deg);
	top: 34px;
}
.navbar-header .navbar-toggle{
	padding: 0;
	margin: 0;
}
.tran .line1{
	top:20px;
	transform: rotate(225deg);
	-ms-transform:rotate(225deg);
	-moz-transform:rotate(225deg);
	-webkit-transform:rotate(225deg);
	-o-transform:rotate(225deg);
}
.tran .line2{
	display: none;
}
.tran .line3{
	top:20px;
	transform: rotate(-225deg);
	-ms-transform:rotate(-225deg);
	-moz-transform:rotate(-225deg);
	-webkit-transform:rotate(-225deg);
	-o-transform:rotate(-225deg);
}

/*****slide*******/

.swiper-container {
	width: 100%;
}
.swiper-slide img{
	width: 100%;
	max-width: 1920px;
	max-height: 827px;
}
.swiper-pagination-bullet {
	width: 40px;
	height: 2px;
	border-radius:0;
	background: #fff;
	opacity: 0.5;
	margin-right: 10px !important;
}
.swiper-pagination-bullet-active{
	background: #fff;
	opacity: 1;
}

/*****video********/
.video{
	padding-bottom: 100px;
}
.video-des{
	text-align: center;
	padding-top: 50px;
}
.video-main{
	padding: 45px 0 0 0;
    text-align: center;
}
.video-main .v{
	padding-top: 45px;
}
.video-main img{
    width: 100%;
    max-width:392px; 
}
.video-main a{
    position: relative;
    display: inline-block;
}
.video-play{
    position: absolute;
    margin-left: -30px;
    margin-top: -65px;
    left: 50%;
    top:50%;
}
.video-play img{
    transition: all .3s ease;
}
.video-play img:hover,.video-bg:hover+.video-play img{
    transform: scale(1.2);
    -ms-transform:scale(1.2);
	-moz-transform:scale(1.2);
	-webkit-transform:scale(1.2);
	-o-transform:scale(1.2);
}
.video-title{
    margin-top:20px;
    text-align: left;
}

